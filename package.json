{
  "name": "action-migrations",
  "version": "1.0.0",
  "private": true,
  "description": "Runs django migration and posts generated SQL in Pull Request",
  "main": "lib/main.js",
  "scripts": {
    "dist": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
